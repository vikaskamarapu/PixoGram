{
  "expo": {
    "name": "InstaCGram",
    "slug": "InstaCGram",
    "owner": "vikaskamarapu5555",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/instagram.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/insta-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/instagram (1).png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.vikaskamarapu5555.InstaCGram"
    },
    "web": {
      "favicon": "./assets/instagram (2).png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f15a30e8-0b4a-4f56-a55e-9ce9fed00807"
      }
    }
  }
}
